{
    "manifest_version": 3,
    "name": "BetterW2G",
    "description": "Because Watch2Gether really needs an updated UI.",
    "version": "1.5",
    "icons": {
        "526": "icon.png"
    },
    "host_permissions": [
        "*://w2g.tv/*"
    ],
    "action": {
        "default_icon": "icon.png",
        "default_popup": "popup.html"
    },
    "content_scripts": [
        {
            "matches": [
                "*://w2g.tv/*"
            ],
            "css": [
                "w2g.css"
            ],
            "run_at": "document_start"
        }
    ]
}
